#Passwordgenerator
import string
import random
def password_generator(lengthofpassword):
    characters=string.ascii_letters+string.ascii_lowercase+string.ascii_uppercase+string.punctuation+string.digits+string.whitespace
    final_password="".join(random.choices(characters,k=lengthofpassword))
    return final_password

length_of_password = 12
displayfinalpassword = password_generator(length_of_password)
print(f"Your strong password is : {displayfinalpassword}")
